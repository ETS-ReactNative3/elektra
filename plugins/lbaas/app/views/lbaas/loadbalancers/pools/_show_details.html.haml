-# .row{data: {update_path: plugin('lbaas').update_status_loadbalancer_path(id: @loadbalancer.id, format: :js), update_interval: 3, update_immediately: true}}
.row
  .col-md-4
    = render partial: 'lbaas/loadbalancers/pools/item_detail', locals: {loadbalancer: @loadbalancer, pool: @pool, listener: @listener }

  .col-md-4
    = render partial: 'lbaas/loadbalancers/pools/healthmonitors/item_nav', locals: {loadbalancer: @loadbalancer, pool: @pool, healthmonitor: @healthmonitor}

  .col-md-1
  .col-md-3
    = render :partial => 'lbaas/loadbalancers/info', locals: {loadbalancer: @loadbalancer}
.row
  .col-md-12
    = render partial: 'lbaas/loadbalancers/pools/members/index', locals: {loadbalancer: @loadbalancer, pool: @pool, members: @members, servers: @servers, statuses: @statuses}
